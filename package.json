{
   "name": "laxar-mocks",
   "version": "2.0.0-pre",
   "description": "Building large applications is hard, so don't do that!",
   "homepage": "http://laxarjs.org",
   "main": "laxar-mocks.js",
   "authors": [],
   "license": "MIT",
   "repository": {
      "type": "git",
      "url": "https://github.com/LaxarJS/laxar-mocks.git"
